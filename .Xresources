 URxvt*scrollBar:                      false
 URxvt*cursorUnderline:                true
 URxvt*cursorBlink:                    true
 URxvt*pointerBlank:                   true

URxvt.perl-ext-common: default,matcher,-searchable-scrollback
URxvt.url-launcher: /bin/chromium
URxvt.matcher.button: 1


URxvt.geometry: 90x21 
URxvt.internalBorder:40
URxvt.depth: 32
URxvt.linespace: 0 
URxvt.letterSpace: 0
!URxvt.font:xft:gohufont:pixelsize=9
!URxvt.boldFont:xft:gohufont:pixelsize=9
!URxvt.italicFont:xft:gohufont:italic:pixelsize=9
URxvt.font:xft:Inconsolata LGC Nerd Font Mono:pixelsize=13
URxvt.boldFont:xft:Inconsolata LGC Nerd Font Mono:pixelsize=13
URxvt.italicFont:xft:Inconsolata LGC Nerd Font Mono:italic:pixelsize=13
!urxvt*font : -gohu-gohufont-medium-r-normal--11-80-100-100-c-60-iso10646-1
!urxvt*boldFont : -gohu-gohufont-bold-r-normal--11-80-100-100-c-60-iso10646-1
!urxvt*italicFont : -gohu-gohufont-bold-r-normal--11-80-100-100-c-60-iso10646-1





!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

URxvt*background:        #333333
URxvt*foreground:        #cccccc
URxvt*cursorColor:       #999999
